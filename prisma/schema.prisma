// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dog {
 id          String   @id @unique @default(uuid())
  name String
  type String
  race String
  description String
  hairType String
  origin String
  size String
}

model Cat {
  id          String   @id @unique @default(uuid())
  name String
  type String
  race String
  description String
  hairType String
  origin String
  size String
}
